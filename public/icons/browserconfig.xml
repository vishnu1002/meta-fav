<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="favicon-48x48.png" />
      <square150x150logo src="favicon-32x32.png" />
      <wide310x150logo src="android-icon-192x192.png" />
      <square310x310logo src="android-icon-512x512.png" />
      <TileColor>#ffffff</TileColor>
    </tile>
    <notification>
      <polling-uri src="/notifications/updates" />
      <frequency>30</frequency>
      <cycle>1</cycle>
    </notification>
  </msapplication>
</browserconfig>
